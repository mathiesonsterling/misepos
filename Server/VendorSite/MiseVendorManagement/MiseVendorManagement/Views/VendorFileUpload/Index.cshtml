@model MiseVendorManagement.Models.VendorViewModel

@{
    ViewBag.Title = "Upload CSV File";
}

<h2>Upload CSV File for @Model.Name</h2>
@using (Html.BeginForm("UploadFile","VendorFileUpload", FormMethod.Post, new { enctype = "multipart/form-data" }))
{ 
    <label for="file">Upload File:</label>
    <input type="file" name="file" id="file" /><br><br>
    <input type="submit" value="Upload Image" />
    <br><br>
    @ViewBag.Message
}
