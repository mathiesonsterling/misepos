@model IEnumerable<MiseReporting.Models.ParLineItemViewModel>

@{
    ViewBag.Title = "Par Items";
}

<h2>Par Items</h2>
<!--
<p>
    @Html.ActionLink("Create New", "Create")
</p>-->
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UPC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContainerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContainerSizeML)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Categories)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UPC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContainerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContainerSizeML)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Categories)
        </td>
        <!--
        <td>
            @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
            @Html.ActionLink("Details", "Details", new {id = item.Id}) |
            @Html.ActionLink("Delete", "Delete", new {id = item.Id})
        </td>-->
    </tr>
}

</table>
